.button.save_remote_images span.wp-media-buttons-icon:before {
	font: 400 18px/1 dashicons;
	speak: none;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
	content: '\f161';
}
.button.qqworld-translation span.wp-media-buttons-icon:before {
	font: 400 18px/1 dashicons;
	speak: none;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
	content: '\f326';
}
#save-remote-images-button {
	-webkit-transition: all .25s;
	-moz-transition: all .25s;
	-o-transition: all .25s;
	-ms-transition: all .25s;
	transition: all .25s;
}
#save-remote-images-button.success {
	-webkit-transform: scale(1.1);
	-moz-transform: scale(1.1);
	-o-transform: scale(1.1);
	-ms-transform: scale(1.1);
	transform: scale(1.1);
}
ul[id^="noty_"] li {
	margin-bottom: 0;
}
#noty_center_layout_container img {
	vertical-align: middle;
}

#remote-images-list.size-1 li {
	width: 233px;
	height: 233px;
}
#remote-images-list.size-2 li {
	width: 172px;
	height: 172px;
}
#remote-images-list.size-3 li {
	width: 135px;
	height: 135px;
}
#remote-images-list li {
	position: relative;
	background-size: cover;
	float: left;
	margin: 5px;
	border: 1px solid #ccc;
	box-shadow: 0 0 0 rgba(0 ,0, 0, .2), inset 0 0 15px rgba(0,0,0,.2);
	position: relative;
	left: 0;
	top: 0;
	-webkit-transition: all .5s, left .5s, border .25s, box-shadow .25s;
	-moz-transition: top .25s, left .25s, border .25s, box-shadow .25s;
	-o-transition: top .25s, left .25s, border .25s, box-shadow .25s;
	-ms-transition: top .25s, left .25s, border .25s, box-shadow .25s;
	transition: top .25s, left .25s, border .25s, box-shadow .25s;
}
#remote-images-list li:hover {
	top: -3px;
	left: -3px;
	box-shadow: 4px 4px 5px rgba(0 ,0, 0, .05);
}
#remote-images-list li div.size {
	position: absolute;
	right: 10px;
	bottom: 10px;
	background: #fff;
	padding: 3px 8px;
	border-radius: 10px;
	box-shadow: inset 0 0 5px rgba(0, 0, 0, .1), 0 0 5px rgba(0, 0, 0, .5);
}
#remote-images-list li span.icon {
	width: 100%;
	height: 100%;
	display: block;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 1;
}
#remote-images-list li span.icon.wait {
	background: url(data:image/gif;base64,) no-repeat center center;
}
#remote-images-list li span.icon.queue {
	background: url(data:image/gif;base64,) no-repeat center center;
}
#remote-images-list li span.icon.right {
	background: url(data:image/png;base64,) no-repeat center center;
}
#remote-images-list li span.icon.deleted {
	background: url(data:image/png;base64,) no-repeat center center;
}
#remote-images-list li span.icon.ignored {
	background: url(data:image/png;base64,) no-repeat center center;
}
#remote-images-list li span.icon.error {
	background: url(data:image/png;base64,) no-repeat center center;
}
#remote-images-list li label {
	width: 100%;
	height: 100%;
	display: block;
}
#remote-images-list li input {
	margin: 5px 0 0 5px;
}
#remote-images-list li.delete {
	left: 0;
	top: 0;
	box-shadow: inset 5px 5px 10px rgba(0,0,0,.1);
}
#remote-images-list li .image-attribute {
	position: absolute;
	left: 0;
	bottom: 0;
	background: #e1e1e1;
	background: rgba(200,200,200,.95);
	width: 100%;
	padding-top: 5px;
	display: none;
}
#remote-images-list li .image-attribute input {
	width: 95%;
	margin-bottom: 5px;
}
#remote-images-list li .image-attribute label span {
	padding: 5px;
}
#remote-images-list li span.btn {
	position: absolute;
	width: 32px;
	height: 32px;
	cursor: pointer;
	display: block;
	filter: alpha(Opacity=75);
	opacity: 0.75;
}
#remote-images-list li span.btn:hover {
	filter:alpha(Opacity=100);
	opacity: 1;
}
#remote-images-list li .edit-image-attributes {
	background: url(../images/edit.png);
	left: 5px;
	bottom: 5px;
}
#remote-images-list li .exclude-image-CRC {
	background: url(../images/exclude.png);
	right: 37px;
	top: 5px;
}
#remote-images-list li .delete-image-CRC {
	background: url(../images/trash.png);
	right: 5px;
	top: 5px;
}

#remote-images-list .close-me {
	display: block;
	background: url(../images/close.png);
	width: 32px;
	height: 32px;
	position: absolute;
	top: -16px;
	right: 5px;
	cursor: pointer;
}

/* for Thick-Box */
#TB_window.qqworld-auto-save-images #TB_ajaxContent {
	width: 100% !important;
	box-sizing: border-box;
	overflow-y: scroll;
}
.qqworld-auto-save-images-control {
	height: 50px;
	line-height: 50px;
	background: #f2f2f2;
	text-align: right;
	padding: 0 20px;
	box-sizing: border-box;
	position: relative;
	box-shadow: inset 0 -5px 15px rgba(0,0,0,.2);
}
.qqworld-auto-save-images-control input.button {
	position: absolute;
	top: 12px;
	right: 15px;
}